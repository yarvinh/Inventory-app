
<div>
    <a href="/brand/<%=@brand.slug%>"><h3> <%=@brand.name%> </h3></a>
    
    <form method="post" action="/sneaker/<%=@brand.slug%>/delete">
    <input type="submit" value="trash">
    <ul>
        <%@sneaker.each do |sneaker|%>
           <li> <input type="checkbox" name="sneaker_ids[]" value="<%= sneaker.id %>" >  <a href="/sneaker/<%=sneaker.id%>">Style: <%=sneaker.style%>, Gender: <%=sneaker.gender%>, Size: <%=sneaker.size%>, QTY: <%=sneaker.quantity%>, Bar Code <%=sneaker.bar_code%> </a></li> 
        <%end%> 
      </ul>   
    </form>
    
</div>